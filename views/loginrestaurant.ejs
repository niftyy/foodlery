<% include ./partials/header %>
<div class="container content">
    <h1>Login Restaurant</h1>
</div>
    <div class="page-wrap container">
        <% include ./partials/messages %>
        <form action="/loginrestaurant" method="POST">
            <div class="form-group">
                    <label for="email">Email</label>
                    <input id="email" class="form-control" type="text" name="email" placeholder="Enter Email">
            </div>
            <div class="form-group">
                    <label for="name">Password</label>
                    <input id="name" class="form-control" type="password" name="password" placeholder="Enter Password">
            </div>
            <br>
            <button class="btn btn-success">Login</button>
        </form>
</div>
<% include ./partials/footer %>
